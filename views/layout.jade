doctype 5
html
  head
    title
      = "Sequelize - "
      = title
    link(rel='stylesheet', href='/stylesheets/style.css')
  body
    .navbar.navbar-default.navbar-fixed-top(role='navigation')
      .navbar-header
        button.navbar-toggle(type='button', data-toggle='collapse', data-target='.navbar-collapse')
          span.sr-only Toggle navigation
          span.icon-bar
          span.icon-bar
          span.icon-bar
        a.navbar-brand(href='/')
          img(src="/images/logo.png")
          span Sequelize
      .navbar-collapse.collapse
        ul.nav.navbar-nav
        ul.nav.navbar-nav.navbar-right
          li.active
            a(href='./') Home
          li
            a(href='../navbar-static-top/') Documentation
          li
            a(href='../navbar-fixed-top/') Articles
          li
            a(href='../navbar-fixed-top/') Changelog
          li
            a(href='../navbar-fixed-top/') Blog
    .main
      .container
        block content
